module LFSR_test(
    );
    parameter k=475;
    reg clk;
    reg rst;
    reg [k-1:0]msg;
    wire [510:0]vout;
    always #3 clk = ~clk;
    initial
    begin
        msg[k-1:0]=475';
        clk=1;
        rst=1;
        #100 rst=~rst;
    end    
    BCH_LFSF out(
        .rst(rst),
        .clk(clk),
        .msg(msg),
        .vout(vout)
        );
    
endmodule
